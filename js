var first_name=("Mary");
var second_name=("Medona");

var full_name=first_name.concat(second_name);

console.log(full_name);

var a="14";
var b="2";

var add=(a+b);
console.log(add);

var sub=(a-b);
console.log(sub);

var mul=(a*b);
console.log(mul);

var div=(a/b);
console.log(div);